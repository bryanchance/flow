<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>io.flow.worker</string>
	<key>LimitLoadToSessionType</key>
	<string>Aqua</string>
	<key>ProgramArguments</key>
	<array>
	  <string>/usr/local/bin/flow-worker</string>
	  <string>-D</string>
	  <string>-c</string>
	  <string>/usr/local/etc/flow.toml</string>

	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>StandardErrorPath</key>
        <string>/dev/null</string>
	<key>StandardOutPath</key>
        <string>/dev/null</string>
</dict>
</plist>
